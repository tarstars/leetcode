import random

class RandomizedSet:

    def __init__(self):
        self.a = []
        self.m = dict()
        

    def insert(self, val: int) -> bool:
        if val in self.m:
            return False
        self.m[val] = len(self.a)
        self.a.append(val)
        return True

    def remove(self, val: int) -> bool:
        if val not in self.m:
            return False
        
        del_ind = self.m[val]
        last_ind = len(self.a) - 1
        if del_ind != last_ind:
            self.a[del_ind], self.a[last_ind] = self.a[last_ind], self.a[del_ind]
            self.m[self.a[del_ind]] = del_ind
            self.m[self.a[last_ind]] = last_ind
        del self.m[self.a.pop()]
        return True

    def getRandom(self) -> int:
        return self.a[random.randint(0, len(self.a) - 1)]
        


# Your RandomizedSet object will be instantiated and called as such:
# obj = RandomizedSet()
# param_1 = obj.insert(val)
# param_2 = obj.remove(val)
# param_3 = obj.getRandom()
